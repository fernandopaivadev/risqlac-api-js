{
  "name": "uirapuru-api",
  "version": "1.0.0",
  "description": "API for the RisqLAB Platform",
  "repository": "https://github.com/FernandoPaivaEC/uirapuru-api",
  "author": "Fernando Paiva <fernandopaiva@techamazon.tech>",
  "license": "COPYRIGHT",
  "private": true,
  "scripts": {
    "update": "yarn upgrade --latest",
    "dev": "nodemon",
    "test": "jest",
    "build": "rm -rf dist && babel src --extensions \".ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "yarn build && npx pm2 start ./dist/index.js",
    "lint": "eslint --fix src/**/*.ts",
    "v": "sh -c 'npm version ${0} -no-git-tag-version'",
    "docker:ps": "sudo docker ps -a",
    "docker:rm": "sh -c 'sudo docker rm ${0} -f'",
    "docker:rmi": "sh -c 'sudo docker rmi ${0} -f'",
    "docker:cli": "sh -c 'sudo docker exec -it ${0} /bin/bash'",
    "docker:logs": "sh -c 'sudo docker-compose logs -f'",
    "docker:reset": "sudo docker rm $(sudo docker ps -a -q) -f && sudo docker rmi $(sudo docker images -a -q) -f",
    "docker:clear": "sudo docker rm $(sudo docker ps -a -q) -f"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.16",
    "@types/nodemailer": "^6.4.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.8.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prisma": "^3.9.1",
    "ts-jest": "^27.1.3",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@prisma/client": "^3.9.1",
    "axios": "^0.25.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.2",
    "socket.io": "^4.4.1",
    "swagger-ui-express": "^4.3.0"
  }
}
