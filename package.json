{
  "name": "risqlac-api",
  "version": "2.0.0",
  "description": "API for the RisqLAC Platform",
  "repository": "https://github.com/FernandoPaivaEC/risqlac-api",
  "author": "Fernando Paiva <fernandopaivaec@gmail.com>",
  "license": "COPYRIGHT",
  "private": true,
  "scripts": {
    "update": "yarn upgrade --latest",
    "test": "jest",
    "dev": "nodemon",
    "lint": "tsc && eslint src",
    "v": "sh -c 'npm version ${0} -no-git-tag-version'",
    "build": "rm -rf dist && babel src --extensions \".ts\" --out-dir dist --copy-files --no-copy-ignored",
    "minify": "sh -c 'minify-all-js ${0}'",
    "deploy": "yarn build && yarn minify dist && prisma generate && serverless deploy --force",
    "start": "yarn build && serverless offline",
    "invoke": "yarn build && sh -c 'serverless invoke local --function ${0}'",
    "debug": "yarn build && export SLS_DEBUG=* && node --inspect ./node_modules/serverless/bin/serverless.js offline -s dev"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.6",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "nodemon": "^2.0.19",
    "prisma": "^4.1.0",
    "serverless": "^3.21.0",
    "serverless-offline": "^9.0.0",
    "serverless-plugin-common-excludes": "^4.0.0",
    "serverless-plugin-include-dependencies": "^5.0.0",
    "serverless-stage-manager": "^1.0.5",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.1.0",
    "@types/cors": "^2.8.12",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "handlebars": "^4.7.7",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.7",
    "serverless-http": "^3.0.1"
  }
}
