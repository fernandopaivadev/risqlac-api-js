{
  "name": "risqlac-api",
  "version": "2.0.0",
  "description": "API for the RisqLAC Platform",
  "repository": "https://github.com/FernandoPaivaEC/risqlac-api",
  "author": "Fernando Paiva <fernandopaivaec@gmail.com>",
  "license": "COPYRIGHT",
  "private": true,
  "scripts": {
    "update": "npm update --save",
    "dev": "nodemon",
    "lint": "tsc && eslint src",
    "v": "sh -c 'npm version ${0} -no-git-tag-version'",
    "build": "rm -rf dist && babel src --extensions \".ts\" --out-dir dist --copy-files --no-copy-ignored",
    "minify": "sh -c 'npx minify-all-js ${0}'",
    "deploy": "npm run build && npm run minify dist && prisma generate && npx serverless deploy --force",
    "start": "npm run build && npx serverless offline",
    "invoke": "npm run build && sh -c 'npx serverless invoke local --function ${0}'",
    "debug": "npm run build && export SLS_DEBUG=* && node --inspect ./node_modules/serverless/bin/serverless.js offline -s dev"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.14",
    "@types/nodemailer": "^6.4.5",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "nodemon": "^2.0.19",
    "prisma": "^4.3.1",
    "serverless": "^3.22.0",
    "serverless-offline": "^9.3.1",
    "serverless-plugin-common-excludes": "^4.0.0",
    "serverless-plugin-include-dependencies": "^5.0.0",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-stage-manager": "^1.0.5",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.8",
    "serverless-http": "^3.0.2"
  }
}
